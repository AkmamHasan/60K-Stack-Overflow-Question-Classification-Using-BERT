                labels=labels\n","            )\n","            loss = outputs.loss\n","            logits = outputs.logits\n","\n","            _, preds = torch.max(logits, dim=1)\n","            y_true.extend(labels.cpu().numpy())\n","            y_pred.extend(preds.cpu().numpy())\n","            correct_predictions += torch.sum(preds == labels)\n","            losses.append(loss.item())\n","\n","\n","\n","            \n","    f1_score = metrics.f1_score(y_true, y_pred, average='weighted')\n","    precision = metrics.precision_score(y_true, y_pred, average='weighted')\n","    recall = metrics.recall_score(y_true, y_pred, average='weighted')\n","    accuracy = correct_predictions.double() / n_examples\n","\n","    print(f'f1 score: {f1_score:.4f}, precision {precision:.4f},recall {recall:.4f}, accuracy {accuracy:.4f}')\n","\n","    print(\"Evaluating model...\")\n","    eval_model(model, val_data_loader, device, len(valid))\n","    \n","    return accuracy, np.mean(losses), precision, recall, f1_score\n"],"metadata":{"id":"LPPjz-OZRjzT","executionInfo":{"status":"ok","timestamp":1683612364399,"user_tz":-360,"elapsed":433,"user":{"displayName":"Akmam Hasan 1911192642","userId":"03457955523335497390"}}},"execution_count":14,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"xl61AMhqnZ_f"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"Yth_RoV2naSO"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"01nZiPQiRj6l"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"1OJqL6kcRkC3"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"_cWB3eIqRkK4"},"execution_count":null,"outputs":[]}]}